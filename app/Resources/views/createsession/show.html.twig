{% extends 'base.html.twig' %}

{% block body %}
    <h1>Createsession</h1>

    <table>
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ createSession.id }}</td>
            </tr>
            <tr>
                <th>Typeofcar</th>
                <td>{{ createSession.typeOfCar }}</td>
            </tr>
            <tr>
                <th>Numberofplaces</th>
                <td>{{ createSession.numberOfPlaces }}</td>
            </tr>
            <tr>
                <th>Address</th>
                <td>{{ createSession.address }}</td>
            </tr>
            <tr>
                <th>Departuretime</th>
                <td>{{ createSession.departureTime }}</td>
            </tr>
        </tbody>
    </table>

    <ul>
        <li>
            <a href="{{ path('createsession_index') }}">Back to the list</a>
        </li>
        <li>
            <a href="{{ path('createsession_edit', { 'id': createSession.id }) }}">Edit</a>
        </li>
        <li>
            {{ form_start(delete_form) }}
                <input type="submit" value="Delete">
            {{ form_end(delete_form) }}
        </li>
    </ul>
{% endblock %}
